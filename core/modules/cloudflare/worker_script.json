{
  "modul": "cloudflare",
  "name": "worker_script",
  "url": "https://www.terraform.io/docs/providers/cloudflare/r/worker_script.html",
  "type": "resource",
  "arguments": [
    {
      "key": "zone",
      "description": "- (Required for single-script accounts) The zone for the script.",
      "type": "nullOr string",
      "default": null
    },
    {
      "key": "name",
      "description": "- (Required for multi-script accounts) The name for the script.",
      "type": "nullOr string",
      "default": null
    },
    {
      "key": "content",
      "description": "- (Required) The script content.",
      "type": "nullOr string",
      "default": null
    }
  ]
}
